        "@stylistic/eslint-plugin": "^2.13.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@tweenjs/tween.js": "^25.0.0",
        "@types/jest": "^29.5.14",
        "@types/leaflet": "^1.9.16",
        "@types/node": "^22.10.5",
        "@types/react": "^19.0.4",
